import matplotlib.pyplot as plt

start = list(map(int, input().split()))
x = []
y = []

for i in range(len(start)):
    if i % 2 == 0:
        x.append(start[i])
    else:
        y.append(start[i])

plt.plot(x, y, 'ro')

plt.title("Number of points")

plt.axis([min(x) - 1, max(x) + 1, min(y) - 1, max(y) + 1])
plt.show()
